// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/prefer-stateless-function": "off",
    "max-len": "off",
    "jsx-a11y/img-uses-alt": "off",
    "no-shadow": "off",
    "no-unused-expressions": "off"
  }
}
